{
  "DOS": {
    "prompt": ["A>", "C:\\>"],
    "commands": {
      "symdeb": {
        "op": "symdeb\r",
        "mode": "prompt"
      }
    }
  },
  "SYMDEB": {
    "prompt": "\n-",
    "commands": {
      "dos": {
        "op": "q\r",
        "mode": "prompt"
      },
      "print": "printf(\"you typed %d\n\", $1)",
      "curstart": "o 3d4 a\ro 3d5 $1\r",
      "curend": "o 3d4 b\ro 3d5 $1\r",
      "curtest": "for i=0 to 13 { curstart $i; wait 200; }",
      "reboot": "a cs:ip\rjmp ffff:0\r\rg\r"
    }
  }
}
